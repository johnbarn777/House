{
    "functions": [
        {
            "source": "functions",
            "codebase": "default",
            "ignore": [
                "node_modules",
                ".git",
                "firebase-debug.log",
                "firebase-debug.*.log",
                "*.local"
            ],
            "predeploy": [
                "npm --prefix \"$RESOURCE_DIR\" run lint"
            ]
        }
    ],
    "flutter": {
        "platforms": {
            "android": {
                "default": {
                    "projectId": "house-backend",
                    "appId": "1:2189201155:android:d6f60ea64705baf1256336",
                    "fileOutput": "android/app/google-services.json"
                }
            },
            "ios": {
                "default": {
                    "projectId": "house-backend",
                    "appId": "1:2189201155:ios:3752abe7be693b48256336",
                    "uploadDebugSymbols": false,
                    "fileOutput": "ios/Runner/GoogleService-Info.plist"
                }
            },
            "dart": {
                "lib/firebase_options.dart": {
                    "projectId": "house-backend",
                    "configurations": {
                        "android": "1:2189201155:android:d6f60ea64705baf1256336",
                        "ios": "1:2189201155:ios:3752abe7be693b48256336"
                    }
                }
            }
        }
    }
}